{
    "name": "entropyphp/entropyphp",
    "description": "Framework EntropyPHP",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Entropy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Entropy\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/willy68/pg-utils.git"
        }
        ],
    "require": {
        "php": ">=8.1",
        "willy68/pg-eventdispacher": "^1.0",
        "php-di/php-di": "^7.0",
        "php-di/invoker": "^2.2",
        "psr/container": "^1.0 || ^2.0",
        "psr/http-server-middleware": "^1.0",
        "guzzlehttp/psr7": "^2.7",
        "willy68/pg-utils": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5 || 11.5 || 12.5",
        "squizlabs/php_codesniffer": "^3.13",
        "symfony/var-dumper": "^7.3"
    },
    "authors": [
        {
            "name": "willy",
            "email": "william.lety@gmail.com"
        }
    ]
}
